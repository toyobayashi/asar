{
  "env": {
    "includePath": [
      "${default}",
      "${workspaceFolder}/include",
      "${workspaceFolder}/deps/jsoncpp/include",
      "${workspaceFolder}/node_modules/@ccpm/oid/include",
      "${workspaceFolder}/node_modules/@ccpm/toyo/include",
      "${workspaceFolder}/node_modules/@ccpm/pickle/include",
      "${workspaceFolder}/node_modules/@ccpm/cmocha/include"
    ],
    "clPath": "C:\\Program Files (x86)\\Microsoft Visual Studio\\2019\\Community\\VC\\Tools\\MSVC\\14.26.28801\\bin\\Hostx64\\x86\\cl.exe",
    "gccPath": "/usr/bin/gcc",
    "clangPath": "/usr/bin/clang"
  },
  "configurations": [
    {
      "name": "Win32",
      "defines": ["_DEBUG", "UNICODE", "_UNICODE"],
      "compilerPath": "${clPath}",
      "windowsSdkVersion": "10.0.18362.0",
      "intelliSenseMode": "msvc-x86",
      "cStandard": "c11",
      "cppStandard": "c++17",
      "includePath": ["${includePath}"]
    },
    {
      "name": "Linux",
      "defines": [],
      "compilerPath": "${gccPath}",
      "cStandard": "c11",
      "cppStandard": "c++17",
      "intelliSenseMode": "gcc-x64",
      "browse": {
        "path": [
          "${workspaceFolder}"
        ],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": ""
      },
      "includePath": ["${includePath}"]
    },
    {
      "name": "macOS",
      "includePath": ["${includePath}"],
      "defines": [],
      "macFrameworkPath": ["/System/Library/Frameworks", "/Library/Frameworks"],
      "compilerPath": "${clangPath}",
      "cStandard": "c11",
      "cppStandard": "c++17",
      "intelliSenseMode": "${default}"
    }
  ],
  "version": 4
}
